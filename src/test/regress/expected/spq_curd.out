CREATE SCHEMA test_curd;
SET search_path TO test_curd;
SET spq.shard_count TO 2;
CREATE TABLE test_dist_curd(
	id INT PRIMARY KEY,
	name TEXT NOT NULL,
	age INT CHECK(age > 0)
);
NOTICE:  CREATE TABLE / PRIMARY KEY will create implicit index "test_dist_curd_pkey" for table "test_dist_curd"
SELECT create_distributed_table('test_dist_curd','id');
